{
	"name": "target_name_basics_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "lake4learningsandbox",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ScheduledTime": {
				"type": "string",
				"defaultValue": "@variables('ScheduledTime')"
			}
		},
		"folder": {
			"name": "Target"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": "name.basics.csv",
				"folderPath": {
					"value": "@concat('csv/',formatDateTime(dataset().ScheduledTime, 'yyyy'),'/',formatDateTime(dataset().ScheduledTime, 'MM'),'/',formatDateTime(dataset().ScheduledTime, 'dd'),'/',\nformatDateTime(convertTimeZone(dataset().ScheduledTime,'UTC','Central Standard Time'), 'HH'),'/',formatDateTime(dataset().ScheduledTime, 'mm'),'/',formatDateTime(dataset().ScheduledTime, 'ss'))",
					"type": "Expression"
				},
				"fileSystem": "imdb-data"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": false,
			"quoteChar": "\""
		},
		"schema": []
	}
}